if not isfolder("PWNED/") then
    makefolder("PWNED/")
end

if not isfile("PWNED/pwned.ogg") then
    writefile("PWNED/pwned.ogg", game:HttpGet("https://github.com/Trollingm/emper/raw/refs/heads/main/skeletons.ogg"))
end

do
    local Lighting = game:GetService("Lighting")
    local StarterGui = game:GetService("StarterGui")
    local SoundService = game:GetService("SoundService")

    local sky = Instance.new("Sky", Lighting)
    sky.SkyboxBk = "rbxassetid://158118263"
    sky.SkyboxDn = "rbxassetid://158118263"
    sky.SkyboxFt = "rbxassetid://158118263"
    sky.SkyboxLf = "rbxassetid://158118263"
    sky.SkyboxRt = "rbxassetid://158118263"
    sky.SkyboxUp = "rbxassetid://158118263"

    local hint = Instance.new("Hint", workspace)
    hint.Text = "Team c00lkidd! Join today!"

    local music = Instance.new("Sound", SoundService)
    music.SoundId = getcustomasset("PWNED/pwned.ogg")
    music.Looped = true
    music.Volume = 1
    music.Pitch = 1
    music:Play()

    coroutine.wrap(function()
        while true do
            for _, sound in pairs(SoundService:GetDescendants()) do
                if sound:IsA("Sound") and sound ~= music then
                    sound.Volume = 0
                end
            end
            wait(0.5)
        end
    end)()

    coroutine.wrap(function()
        while true do
            StarterGui:SetCore("SendNotification", {
                Title = "Team c00lkidd attacks the game!",
                Text = "TEAM C00LKIDD JOIN TODAY!",
                Duration = 2,
                Icon = "rbxassetid://158118263"
            })
            wait(2)
        end
    end)()
end

do
    local decalID = 158118263

    local function exPro(root)
        for _, v in pairs(root:GetChildren()) do
            if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id="..decalID then
                v.Parent = nil
            elseif v:IsA("BasePart") and v.Transparency == 0 then
                v.Material = Enum.Material.Plastic
                for _, d in pairs(v:GetChildren()) do
                    if d:IsA("Decal") then d:Destroy() end
                end
                local faces = {"Front", "Back", "Right", "Left", "Top", "Bottom"}
                for _, face in pairs(faces) do
                    local newDecal = Instance.new("Decal", v)
                    newDecal.Texture = "http://www.roblox.com/asset/?id="..decalID
                    newDecal.Face = Enum.NormalId[face]
                end
            end
            exPro(v)
        end
    end

    local function asdf(root)
        for _, v in pairs(root:GetChildren()) do
            asdf(v)
        end
    end

    exPro(game.Workspace)
    asdf(game.Workspace)

    local s = Instance.new("Sky")
    s.Name = "Sky"
    s.Parent = game.Lighting
    local skyboxID = 158118263
    s.SkyboxBk = "http://www.roblox.com/asset/?id="..skyboxID
    s.SkyboxDn = "http://www.roblox.com/asset/?id="..skyboxID
    s.SkyboxFt = "http://www.roblox.com/asset/?id="..skyboxID
    s.SkyboxLf = "http://www.roblox.com/asset/?id="..skyboxID
    s.SkyboxRt = "http://www.roblox.com/asset/?id="..skyboxID
    s.SkyboxUp = "http://www.roblox.com/asset/?id="..skyboxID
    game.Lighting.TimeOfDay = 12

    for _, v in pairs(game.Players:GetPlayers()) do
        if v.Character then
            local torso = v.Character:FindFirstChild("Torso")
            if torso then
                local emit = Instance.new("ParticleEmitter", torso)
                emit.Texture = "http://www.roblox.com/asset/?id="..skyboxID
                emit.VelocitySpread = 20
            end
        end
    end
end

do
    local gui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    local screenGui = Instance.new("ScreenGui", gui)

    local function mk(pos, xdir, ydir)
        local i = Instance.new("ImageLabel", screenGui)
        i.Size = UDim2.new(0, 150, 0, 150)
        i.Position = pos
        i.BackgroundTransparency = 1
        i.Image = "rbxassetid://158118263"
        local xs, ys = 600, 450
        local lx, ly = tick(), tick()
        local xd, yd = xdir, ydir
        game:GetService("RunService").RenderStepped:Connect(function()
            local t = tick()
            local dt = t - lx
            lx = t
            local p = i.Position
            local x, y = p.X.Offset + xs * xd * dt, p.Y.Offset + ys * yd * dt
            local s = workspace.CurrentCamera.ViewportSize
            if x < 0 then
                x, xd = 0, 1
            elseif x + 150 > s.X then
                x, xd = s.X - 150, -1
            end
            if y < 0 then
                y, yd = 0, 1
            elseif y + 150 > s.Y then
                y, yd = s.Y - 150, -1
            end
            i.Position = UDim2.new(0, x, 0, y)
        end)
    end

    mk(UDim2.new(0, 0, 0, 0), 1, 1)
    wait(5)
    mk(UDim2.new(1, -150, 1, -150), -1, -1)
end
